<script type="text/javascript">
(function() {
    var back_redirect_back_link = #'https://oferta.tecnicassecretas.site/';#
    var blockRedirect = false;
    var isMobile = window.innerWidth <= 768 || /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent);
    
    function getQueryString() {
        return window.location.search || '';
    }
    
    // GARANTIR que n√£o h√° beforeunload
    window.onbeforeunload = null;
    window.removeEventListener('beforeunload', function() {});
    
    // Push history states
    history.pushState({}, "", location.href);
    history.pushState({}, "", location.href);
    if (isMobile) {
        history.pushState({}, "", location.href);
        history.pushState({}, "", location.href);
    }
    
    // MOBILE: Swipe detection
    var xDown = null, yDown = null;
    
    document.addEventListener('touchstart', function(evt) {
        if (blockRedirect) return;
        xDown = evt.touches[0].clientX;
        yDown = evt.touches[0].clientY;
    }, {passive: false});
    
    document.addEventListener('touchmove', function(evt) {
        if (!xDown || !yDown || blockRedirect) return;
        
        var xUp = evt.touches[0].clientX;
        var xDiff = xDown - xUp;
        
        if (Math.abs(xDiff) > 100 && xDiff < 0) {
            evt.preventDefault();
            
            var queryString = getQueryString();
            var finalUrl = back_redirect_back_link + queryString;
            
            // REDIRECT DIRETO - EXPERI√äNCIA SUAVE
            window.location.href = finalUrl;
            
            xDown = null;
            yDown = null;
            return false;
        }
    }, {passive: false});
    
    document.addEventListener('touchend', function() {
        xDown = null;
        yDown = null;
    }, {passive: true});
    
    // BOT√ÉO VOLTAR - REDIRECT DIRETO
    window.onpopstate = function() {
        if (blockRedirect) {
            console.log('Redirect bloqueado - bot√£o clicado');
            return;
        }
        
        var queryString = getQueryString();
        var finalUrl = back_redirect_back_link + queryString;
        
        // REDIRECT DIRETO - SEM DELAY, SEM AVISO
        setTimeout(function() {
            location.href = finalUrl;
        }, 0);
    };
    
    // Exclus√£o de bot√µes (mesmo c√≥digo anterior)
    function shouldExcludeElement(element) {
        if (!element) return false;
        
        var currentEl = element;
        while (currentEl && currentEl !== document.body) {
            if (currentEl.tagName === 'BUTTON' || currentEl.tagName === 'A') return true;
            
            var href = currentEl.getAttribute('href') || '';
            if (href && (
                href.includes('checkout') ||
                href.includes('whatsapp') ||
                href.includes('wa.me') ||
                href.includes('hotmart') ||
                href.includes('kiwify') ||
                href.includes('pay') ||
                href.startsWith('http')
            )) return true;
            
            var className = currentEl.className || '';
            if (className.includes('btn') || 
                className.includes('button') || 
                className.includes('cta') ||
                className.includes('no-redirect')) return true;
            
            currentEl = currentEl.parentNode;
        }
        return false;
    }
    
    document.addEventListener('click', function(e) {
        if (shouldExcludeElement(e.target)) {
            blockRedirect = true;
            console.log('‚úÖ Bot√£o exclu√≠do do redirect:', e.target);
            
            setTimeout(function() {
                blockRedirect = false;
            }, 3000);
        }
    }, true);
    
    console.log('üöÄ Redirect DIRETO ativo - Experi√™ncia otimizada!');
    
})();
</script>









